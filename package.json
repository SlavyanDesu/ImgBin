{
  "name": "tempstorage",
  "version": "1.6.1",
  "description": "A simple, fast, and temporary file storage solution.",
  "main": "src/server.ts",
  "scripts": {
    "dev": "nodemon",
    "build": "rimraf dist && tsc && npm run copy-public && npm run copy-views",
    "copy-public": "cpx \"src/public/**/*\" \"dist/public\"",
    "copy-views": "cpx \"src/views/**/*\" \"dist/views\"",
    "lint": "eslint .",
    "postinstall": "prisma generate",
    "start": "node dist/server.js",
    "prettier": "npx prettier . --write",
    "predeploy": "npm run build && npm run lint && npm run prettier"
  },
  "keywords": [],
  "author": "SlavyanDesu",
  "license": "MIT",
  "dependencies": {
    "@prisma/client": "^6.4.1",
    "cloudinary": "^2.5.1",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "ejs": "^3.1.10",
    "express": "^4.21.2",
    "multer": "^1.4.5-lts.1",
    "serve-favicon": "^2.5.0",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.21.0",
    "@types/cookie-parser": "^1.4.8",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/express-session": "^1.18.1",
    "@types/multer": "^1.4.12",
    "@types/serve-favicon": "^2.5.7",
    "cpx": "^1.5.0",
    "eslint": "^9.21.0",
    "globals": "^16.0.0",
    "nodemon": "^3.1.9",
    "pre-commit": "^1.2.2",
    "prettier": "3.5.2",
    "prettier-plugin-ejs": "^1.0.3",
    "prisma": "^6.4.1",
    "rimraf": "^6.0.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.26.0"
  }
}
